{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "": "babel-node",
    "js": "node --harmony"
  },
  "watch": [
    "apis/*.js",
    "app.js",
    "data_manage/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json jade"
}
